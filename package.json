{
  "name": "next-template",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:seed": "ts-node prisma/seed.ts",
    "db:clear": "tsx scripts/clear-database.ts",
    "pre-commit": "npm run format:check && npm run lint && npm run typecheck && npm run test",
    "quality-check": "npm run format:check && npm run lint && npm run typecheck && npm run test && npm run build",
    "fix-all": "npm run format:write && npm run lint:fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write --cache",
      "eslint --fix"
    ],
    "*.{js,jsx}": [
      "prettier --write --cache",
      "eslint --fix"
    ],
    "*.{json,md,mdx}": [
      "prettier --write --cache"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@aws-sdk/s3-request-presigner": "^3.832.0",
    "@hookform/resolvers": "^5.1.1",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/google.maps": "^3.58.1",
    "@vercel/analytics": "^1.5.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.522.0",
    "next": "^15.3.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "prisma": "^6.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.59.0",
    "react-resizable-panels": "^3.0.3",
    "react-time-picker": "^7.0.0",
    "sharp": "^0.34.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/mailchimp__mailchimp_marketing": "^3.0.21",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "6.4.17",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/parser": "^8.29.0",
    "cross-fetch": "^4.1.0",
    "eslint": "8.57.1",
    "eslint-config-next": "^15.3.4",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-tailwindcss": "^4.0.0-alpha.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.4.24",
    "prettier": "^3.6.0",
    "supabase": "^2.26.9",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "eslint": "8.57.1"
  }
}
