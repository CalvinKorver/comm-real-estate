{
  "name": "next-template",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "test": "vitest run --config vitest.config.mjs",
    "test:watch": "vitest --config vitest.config.mjs",
    "test:coverage": "vitest run --coverage --config vitest.config.mjs",
    "test:integration": "vitest run --config vitest.config.integration.mjs",
    "test:integration:watch": "vitest --config vitest.config.integration.mjs",
    "test:integration:ui": "vitest --config vitest.config.integration.mjs --ui",
    "test:db:setup": "docker-compose -f docker-compose.test.yml up -d --wait",
    "test:db:teardown": "docker-compose -f docker-compose.test.yml down -v",
    "db:seed": "ts-node prisma/seed.ts",
    "db:clear": "tsx scripts/clear-database.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
